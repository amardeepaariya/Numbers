import java.util.Scanner;
 class EvenOdd1     
 {
	 public static void main(String[] args)     //without using %(modules)operator
	 {
		 Scanner sc=new Scanner(System.in);
		 System.out.println("Enter the No.");
		 int n=sc.nextInt();
		 if(n/2*2==n)
			 System.out.println(n+"=The No. is Even");
		 else
			 System.out.println(n+"=The No. is odd");
	 }
 }
